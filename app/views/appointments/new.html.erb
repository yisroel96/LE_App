<h1>Create a Product</h1>

<%= form_for @appointment do |f| %>
  <%= f.label :date%>
  <%= f.text_field :date%><br>
  <%= f.label :time%>
  <%= f.text_field :time%><br>
  <%= f.label :client %>
  <%= f.collection_select :client_id, Client.all, :id, :name %>
  <%= f.label :le %>
  <%= f.collection_select :le_id, Le.all, :id, :first_name %>
  <br />

  <%= f.submit %>
<% end %>
